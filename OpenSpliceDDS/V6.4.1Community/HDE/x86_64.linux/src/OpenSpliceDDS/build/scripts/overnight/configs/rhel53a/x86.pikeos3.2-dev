SETUP_TYPE=x86.pikeos3-dev
BUILD_DIST=yes
BUILD_EXAMPLES=yes
KEEP_DIST=yes
BUILD_DBT=yes
BUILD_RBT=yes
RUN_DBT=no
RUN_RBT_SP=no
RUN_RBT_SHM=no
RUN_EXAMPLES_SHM=no
RUN_EXAMPLES_SP=no

. configs/`hostname`/common

PIKEOS_HOME=/opt/pikeos-3.2
PATH=$PATH:$PIKEOS_HOME/cdk/x86/i586/bin

unset TAO_ROOT
unset MICO_ROOT
unset MICO_LIBS
unset QTDIR